#include <iostream>
#include <thread>


void func1()
{
	std::cout << "hello \n";

}


void func2() 
{
	std::cout << "Second \n";
}

void func3(void *data)
{
	char *str = (char *)data;
	std::cout << "Third with Argument :"<< data;

}


int main()
{
	std::thread t(func3, "Hi");
	t.join();
	return 0;
}
